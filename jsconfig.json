{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "config/*": ["./src/config/*"],
      "navigation/*": ["./src/navigation/*"],
      "redux/*": ["./src/redux/*"],
      "screens/*": ["./src/screens/*"],
      "styles/*": ["./src/styles/*"],
      "tabScreens/*": ["./src/tabScreens/*"],
      "utils/*": ["./src/utils/*"]
    }
  },
  "include": ["src", "App.js"],
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
